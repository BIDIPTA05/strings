#include<iostream>
using namespace std;
int main(){
    string a = "hellto";
    string ne;
    // for (int i = a.size() - 1; i >= 0;i--){
    //     ne.push_back(a[i]);
    // }

    int low = 0;
    int high = a.size() - 1;
    while(low<high){
        swap(a[low], a[high]);
        low++;
        high--;
    }
    cout << a;

    return 0;
}
